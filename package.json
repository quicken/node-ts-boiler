    "dotenv": "^16.0.0",
    "esbuild": "^0.15.12",
    "eslint": "^8.11.0",
    "jest": "^29.2.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3",
