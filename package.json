{
  "name": "changeme",
  "version": "1.0.0",
  "description": "Command Line Project Template using Typescript",
  "license": "MIT",
  "author": "Marcel Scherzer",
  "homepage": "https://www.herdingbits.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/quicken/node-ts-boiler.git"
  },
  "keywords": [
    "herdingbits",
    "typescript",
